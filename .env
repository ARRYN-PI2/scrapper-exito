# Environment configuration for Exito Scraper

# Python versions for CI/CD testing
PYTHON_VERSION_MAIN=3.11
PYTHON_VERSION_MIN=3.9
PYTHON_VERSION_MAX=3.12
PYTHON_VERSIONS_JSON='["3.9", "3.10", "3.11", "3.12"]'

# Docker configuration
DOCKER_IMAGE_NAME=exito-scraper
DOCKER_TAG=latest

# Application configuration
APP_NAME=exito-scraper
APP_VERSION=1.0.0
APP_DESCRIPTION="Exito E-commerce Web Scraper"

# Scraper default settings
DEFAULT_TIMEOUT=30
DEFAULT_DELAY=2
DEFAULT_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"

# Output configuration
DEFAULT_OUTPUT_DIR=data
DEFAULT_OUTPUT_FORMAT=jsonl

# CI/CD configuration
CODECOV_TOKEN=your-codecov-token-here
GITHUB_TOKEN=your-github-token-here
